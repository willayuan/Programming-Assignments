#include <iostream>

using namespace std;

int main()
{
    cout << "Enter a number: ";
    int x;
    cin >> x;
   for (int i=2; i < x; i++)
   {
       if (x%i==0)
       {
           cout << x << " is not prime" << endl;
           return 0;
       }
   }
    cout << x << " is prime" << endl;
    return 0;
}


